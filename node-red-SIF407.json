    {
        "id": "5874228ec126bc70",
        "type": "function",
        "z": "6707720b260a7de7",
        "name": "function 1",
        "func": "\nlet dataArray = msg.payload.Object.Locations; \n\n\n\nlet influxPoints = dataArray.map(item => {\n    if (!item) return null;\n\n    let skuObject = item[\"SKU\"];\n    let skuValue = (skuObject && typeof\n        skuObject === 'object')\n        ? String(skuObject.SKUID || \"Unknown SKU Missing ID\")\n        : String(skuObject || \"Unknown SKU Missing ID\");\n    if (!Array.isArray(dataArray) || dataArray.length === 0) {\n        node.warn(\"Errorã€‚\");\n        return null;\n    }\n        \n   \n    let statusTextValue = (item[\"LocationStatus\"] && typeof item[\"LocationStatus\"] === 'object') \n                          ? String(item[\"LocationStatus\"].Description) \n                          : String(item[\"LocationStatus\"] || \"Unknown_Status\");\n\n    return [\n        // (Field\n        {\n            \"status_id\": Number(item[\"LocationStatusID\"]), \n            \"row\": Number(item[\"Row\"]),\n            \"column\": Number(item[\"Column\"]),\n            \"disabled\": (item[\"Disabled\"] === \"true\" || item[\"Disabled\"] === true) ? true : false,\n            \"position_number\": Number(item[\"PositionNumber\"]),\n            \"position_label\": String(item[\"PositionLabel\"] || \"N/A\") \n        },\n        // Tags\n        {\n            \"location_id\": String(item[\"LocationID\"]), \n            \"station_id\": String(item[\"StationID\"]),\n            \n            \"sku\": skuValue, \n            \"location_status_text\": statusTextValue \n        }\n    ];\n}).filter(point => point !== null); \n\nmsg.payload = influxPoints;\nmsg.measurement = \"warehouse_location\"; \n\nif (msg.payload.length === 0) {\n    return null;\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
